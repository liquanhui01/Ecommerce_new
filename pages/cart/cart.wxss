page{
  margin: 0;
  padding: 0;
}

.total_page{
  width: 100%;
  height: 100%;
}

.top{
  width: 100%;
  height: 90%;
  margin-bottom: 15%;
}

.bottom{
  width: 100%;
  height: 10%;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  background-color: white;
}

.item{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.total{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  background-color: #fafafa;
}

.button{
  width: 65px;
  height: inherit;
  display: flex;
  flex-direction: column;
  text-align: center;
  justify-content: center;
  color: white;
}

.cancelButton{
  width: 65px;
  height: inherit;
  display: flex;
  flex-direction: column;
  text-align: center;
  justify-content: center;
  color: yellow;
}

.left{
  background-color: #2E8B57;
}

.middle{
  background-color: red;
}

.right{
  background-color: yellow;
}

.all_selected{
  flex: 1;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding-left: 20rpx;
}

.all_selected_title{
  margin-left: 5rpx;
  font-size: 28rpx;
}

.active{
  border: 1rpx solid red;
}

.total_price{
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-weight: 400;
}

.total_price_content{
  font-size: 37rpx;
  color: red;
}

.bottom_button{
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  padding-right: 30rpx;
}